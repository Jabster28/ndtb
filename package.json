{
  "name": "ndtb",
  "version": "1.0.0",
  "description": "A bot made for finding loops for the 'New DT Cannon'",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "license": "MIT",
  "keywords": [],
  "scripts": {
    "test": "yarn run check && yarn run unit",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc && node build/src/closure.js",
    "docs": "jsdoc -c jsdoc.json",
    "fix": "gts fix",
    "go": "yarn compile && node build/src/compiledIndex.js",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run check",
    "start": "tsc && node build/src/index.js",
    "unit": "nyc --reporter=lcov --reporter=text mocha",
    "coverage": "codecov"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/mocha": "8.2.3",
    "@types/node": "13.13.52",
    "axios": "0.28.0",
    "better-docs": "2.3.2",
    "codecov": "3.8.2",
    "eslint-plugin-mocha": "7.0.1",
    "gts": "2.0.2",
    "jsdoc": "3.6.7",
    "mocha": "8.4.0",
    "nyc": "15.1.0",
    "querystring": "0.2.1",
    "taffydb": "2.7.3",
    "typescript": "4.0.3"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/socket.io": "^2.1.8",
    "colors": "^1.4.0",
    "express": "^4.17.1",
    "http": "^0.0.1-security",
    "robotjs": "^0.6.0",
    "socket.io": "^2.3.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  }
}
